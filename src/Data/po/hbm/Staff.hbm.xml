<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="Data.po.User.Staff" table="staff" schema="dbo" catalog="restaurant">
        <id name="id">
            <column name="id" sql-type="varchar(18)" length="18" not-null="true"/>
        </id>
        <property name="account">
            <column name="account" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="position">
            <column name="position" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="pay">
            <column name="pay" sql-type="float" precision="-1" not-null="true"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>