<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="Data.po.User.TopupDetail" table="TopupDetail" schema="dbo" catalog="restaurant">
        <id name="id">
            <column name="id" sql-type="int"/>
            <generator class="native"/>
        </id>
        <property name="account">
            <column name="account" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="number" >
            <column name="number" sql-type="float"/>
        </property>
        <property name="time" >
            <column name="time" sql-type="varchar(50)" length="50"/>
        </property>
    </class>
</hibernate-mapping>