<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Data.po.goods.DishInMenu" table="menu" schema="dbo" catalog="restaurant">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="num">
            <column name="num" sql-type="int" not-null="true"/>
        </property>
        <property name="prize">
            <column name="prize" sql-type="float" precision="-1" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>